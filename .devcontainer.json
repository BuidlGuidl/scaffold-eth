{
  "name": "Node.js",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "contractshark.solidity-lang@1.3.0"
  ],
  "forwardPorts": [
    3000
  ],
  "hostRequirements": {
    "memory": "8gb"
  },
  "remoteUser": "node",
  "image": "ghcr.io/buidlguidl/scaffold-eth:sha-87944a8",
  "containerEnv": {
     "REACT_APP_CODESPACES": "true"
  },
  "postCreateCommand": "cp /workspaces/scaffold-eth-tmp/node_modules /workspaces/scaffold-eth/node_modules && yarn install"
}